{    
  "manifest_version": 2,  
  "name": "mytime",  
  "description": "Extension for mytime",  
  "version": "1.0",  
  "browser_action": {  
   "default_icon": "icon/logo.png",  
   "default_popup": "index.html"  
  }, 
"background": {
       "scripts": ["background.js"],
       "persistent" :false
     },

  "permissions": [  
   "activeTab",  
   "storage","alarms"
  ]  
 
 }  